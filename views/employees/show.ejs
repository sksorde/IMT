<!DOCTYPE html>
<html>
<head>
    <title>Employee Detail</title>
    <link rel='stylesheet'
          href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/toolbar.css' />
    <link rel='stylesheet' href='/stylesheets/DetailFrame.css' />
</head>
<body>


    <div class="topnav">
        <a class="active" , href="/home">Home</a>
        <a href="/incident/create">Create New Incident</a>
        <a href="/incident">Open Incident</a>
        <a href="/ply">PLY</a>
        <a href="#has">HAS</a>
        <a href="#Search">Search</a>
        <input type="text"
               placeholder="Site Search...">
    </div>
    <div class="sidenav">
        <a href="/home">IMT-Home</a>
        <a href="/incident/create">Create New Incident</a>
        <a href="/incident">Open Incidents</a>
        <a href="#QuickList">Quick List</a>
        <a href="#TTT">Transitory Team Referral</a>
        <a href="#over21days">
            Open

            Over 21 days
        </a>
        <a href="#AAC-consulation">AAC Consultation</a>
        <a href="/Stage3">Stage3</a>
        <a href="#Segment 5">Segment 5</a>
        <a href="#LocalTriage">Local Triage</a>
    </div>


    <div class="container">


        <h1>Incident Detail</h1>
        <table>
            <tbody>
                <tr>
                    <td>Raised By</td>

                    <td>:</td>
                    <td><%= employee.raisedby %></td>
                </tr>
                <tr>
                    <td>Location</td>

                    <td>:</td>
                    <td><%= employee.location %></td>
                </tr>
                <tr>

                    <td>Team Leader</td>
                    <td>:</td>
                    <td><%= employee.teamleader%></td>
                </tr>
		<tr>

                    <td>Team</td>
                    <td>:</td>
                    <td><%= employee.team%></td>
                </tr>
                <tr>

                    <td>Sub Area</td>
                    <td>:</td>
                    <td><%= employee.subarea%></td>
                </tr>

                <tr>

                    <td>SR Number</td>
                    <td>:</td>
                    <td><%= employee.srnumber%></td>
                </tr>

                <tr>

                    <td>SR Sub Status</td>
                    <td>:</td>
                    <td><%= employee.srsubstatus%></td>
                </tr>

                <tr>

                    <td>Backgound Information </td>
                    <td>:</td>
                    <td><%= employee.bginfo%></td>
                </tr>                

		 


            </tbody>
        </table>
        <h3><a href="/employees/edit/<%= employee._id%>">Add New Update</a></h3>

	

    </div>

	





<div class="detailframe">
		
		<% if(employeedetail.length>0) { %>
        <table class="list-table">
           <thead>
                <tr>
		    <th>Updated Date</th>
                    <th>Details</th>
                    
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i
                <employeedetail.length;i++) { %>
                    <tr>
			<td><%= employeedetail[i].updated_at%></td>                                                
                        <td><%= employeedetail[i].otherupdates%></td>
			<td
                    </tr>
                    <% } %>
            </tbody>
        </table>
        <% } else { %>




       
        <% } %>


	</div>

</body>
</html>
